@model List<Category>

<h1>Category view</h1>

<table class="table table-bordered table-striped" :
     <thead>
        <tr>
            <th>
                Category Name
            </th>
            <th>
                Display order
            </th>
        </tr>
     </thead> 
     <tbody>
        @foreach(var obj in Model.OrderBy(u=>u.DisplayOrder))
        {
             <tr>
                <th>
                    @obj.Name
                </th>
                <th>
                    @obj.DisplayOrder

                </th>
              </tr>
        }
     </tbody>

</table>